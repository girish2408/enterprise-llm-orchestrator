{
  "name": "enterprise-llm-orchestrator",
  "version": "1.0.0",
  "description": "Enterprise LLM orchestrator with MCP tools, LangChain agent, and PostgreSQL persistence",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json && prisma generate",
    "start": "prisma migrate deploy && node dist/index.js",
    "start:prod": "NODE_ENV=production prisma migrate deploy && node dist/index.js",
    "lint": "eslint .",
    "test": "vitest run",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "llm",
    "mcp",
    "langchain",
    "postgresql",
    "enterprise"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.1.63",
    "@langchain/openai": "^0.0.14",
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "langchain": "^0.1.0",
    "openai": "^4.24.7",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "tsx": "^4.6.2",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
